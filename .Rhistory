if(!(state%in%states)){stop("invalid state")}
if(!(outcome%in%outcomes)) {stop("invalid outcome")}
#best and worst
if (num == "best") {num <- 1}
if (num == "worst") {num <- length(data[states==state & data[,outcome]!="NA",])}
if (num > length(data[states==state & data[,outcome]!="NA",])) {num <- return(NA)}
